@model EditForumPostModel
@{   
    Layout = "_ColumnsOne";

    Html.AddTitleParts(T("Forum.PageTitle.PostNew").Text);
}
@await Component.InvokeAsync("Widget", new { widgetZone = PublicWidgetZones.BoardsPostCreateBefore, additionalData = Model })

@await Html.PartialAsync("_CreateUpdatePost", Model)

@await Component.InvokeAsync("Widget", new { widgetZone = PublicWidgetZones.BoardsPostCreateAfter, additionalData = Model })
